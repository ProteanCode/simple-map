{
    "name": "yarbala/simple-map",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "laravel/nova": "*",
        "laravel/nova-devtool": "^1.6"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../packages/laravel/nova"
        }
    ],
    "autoload": {
        "psr-4": {
            "Yarbala\\SimpleMap\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yarbala\\SimpleMap\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
